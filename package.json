{
  "name": "preact-boilerplate",
  "version": "0.0.0",
  "description": "A lightweight and modern starter template for building web applications using Preact and Vite with fast builds, hot module replacement, and production-ready configurations",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexferl/preact-boilerplate.git"
  },
  "author": "alexferl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexferl/preact-boilerplate/issues"
  },
  "homepage": "https://github.com/alexferl/preact-boilerplate#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "coverage:ui": "vitest run --coverage && open coverage/index.html",
    "lint": "biome check --write"
  },
  "dependencies": {
    "preact": "10.28.2",
    "preact-iso": "2.11.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@preact/preset-vite": "2.10.2",
    "@testing-library/preact": "3.2.4",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "jsdom": "27.4.0",
    "vite": "7.3.1",
    "vitest": "4.0.17"
  }
}
